    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "helmet": "^3.6.1",
    "history": "^4.6.3",
    "lodash.merge": "^4.6.0",
    "lodash.union": "^4.6.0",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "re-reselect": "^0.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
