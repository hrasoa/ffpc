{
  "name": "react-pwa",
  "version": "1.0.0",
  "description": "React progressive web application",
  "main": "src/index.js",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production APP_ENV=prod npm run build",
    "build": "concurrently --kill-others-on-fail \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack --config webpack/client.build.js",
    "build:backend": "babel server -d serverBuild",
    "build:render": "webpack --config webpack/server.build.js",
    "build:server": "npm run build:render && npm run build:backend",
    "clean": "rimraf clientBuild/* serverBuild/*",
    "prebuild": "npm run clean",
    "prestart:dev": "npm run clean",
    "start": "NODE_ENV=production APP_ENV=prod node serverBuild/index.js",
    "start:dev": "NODE_ENV=development APP_ENV=dev babel-watch -L -x ./src server/index.js",
    "test": "jest"
  },
  "repository": "git+https://github.com/hrasoa/react-pwa.git",
  "author": "Haja Rasoahaingo <haja.rasoah@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hrasoa/react-pwa/issues"
  },
  "homepage": "https://github.com/hrasoa/react-pwa#readme",
  "dependencies": {
